<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.question0_3.view.SettingOfGame"
            prefHeight="400.0" prefWidth="600.0">
    <center>
        <VBox spacing="10" alignment="CENTER">
            <Label text="levels: "/>
            <ComboBox fx:id="levelSelect" promptText="second degree"/>
            <Label text="Do you want to have audio or want to mute?"/>
            <ComboBox fx:id="audio" promptText="yes"/>
            <Button text="start" onMouseClicked="#runGame"/>
        </VBox>
    </center>
</BorderPane>
